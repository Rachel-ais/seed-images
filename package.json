{
    "name": "seed-images",
    "version": "4.0.4",
    "description": "Angular component for Seed image discovery",
    "scripts": {
        "ng": "ng",
        "start": "npm-run-all --parallel ngserve test lint",
        "ngserve": "ng serve --host 0.0.0.0 --port 8080 --disable-host-check",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean": "rimraf .tmp && rimraf dist",
        "transpile": "ngc -p ./src/tsconfig.build.json",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/seed-images.umd.js --screw-ie8 --compress --mangle--comments--output dist/bundles/seed-images.umd.min.js",
        "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/* && cpx './readme.md' dist",
        "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AppliedIS/seed-images.git"
    },
    "bugs": {
        "url": "https://github.com/AppliedIS/seed-images/issues"
    },
    "homepage": "https://github.com/AppliedIS/seed-images#readme",
    "author": "Applied Information Sciences",
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/animations": "^5.2.8",
        "@angular/common": "^5.2.8",
        "@angular/compiler": "^5.2.8",
        "@angular/core": "^5.2.8",
        "@angular/forms": "^5.2.8",
        "@angular/http": "^5.2.8",
        "@angular/platform-browser": "^5.2.8",
        "@angular/platform-browser-dynamic": "^5.2.8",
        "@angular/router": "^5.2.8",
        "clipboard": "^2.0.0",
        "core-js": "^2.5.3",
        "font-awesome": "^4.7.0",
        "js-beautify": "^1.7.5",
        "primeng": "^5.2.3",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.20"
    },
    "devDependencies": {
        "@angular/cli": "1.6.1",
        "@angular/compiler-cli": "^5.2.8",
        "@angular/language-service": "^5.2.8",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "^4.0.1",
        "cpx": "^1.5.0",
        "jasmine-core": "^2.8.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.3.0",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "npm-run-all": "^4.1.2",
        "protractor": "^5.2.1",
        "renamer": "^0.6.1",
        "rimraf": "^2.6.2",
        "rollup": "^0.52.1",
        "ts-node": "^4.0.1",
        "tslint": "^5.8.0",
        "typescript": "^2.4.2",
        "uglify-js": "^3.2.2"
    }
}
