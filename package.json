{
    "name": "@ngageoint/seed-images",
    "version": "4.6.1",
    "description": "Angular component for Seed image discovery",
    "scripts": {
        "ng": "ng",
        "start": "npm-run-all --parallel ngserve lint",
        "ngserve": "ng serve --host 0.0.0.0 --port 8080 --disable-host-check",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean": "rimraf .tmp && rimraf dist",
        "transpile": "ngc -p ./src/tsconfig.build.json",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/seed-images.umd.js --screw-ie8 --compress --mangle--comments--output dist/bundles/seed-images.umd.min.js",
        "copy": "cp './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/* && cp './readme.md' dist",
        "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy",
        "release": "standard-version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ngageoint/seed-images.git"
    },
    "bugs": {
        "url": "https://github.com/ngageoint/seed-images/issues"
    },
    "homepage": "https://github.com/ngageoint/seed-images#readme",
    "author": "Applied Information Sciences",
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/animations": "^7.2.15",
        "@angular/cdk": "^7.3.7",
        "@angular/common": "^7.2.15",
        "@angular/compiler": "^7.2.15",
        "@angular/core": "^7.2.15",
        "@angular/forms": "^7.2.15",
        "@angular/http": "^7.2.15",
        "@angular/platform-browser": "^7.2.15",
        "@angular/platform-browser-dynamic": "^7.2.15",
        "@angular/router": "^7.2.15",
        "clipboard": "^2.0.4",
        "core-js": "^2.6.5",
        "font-awesome": "^4.7.0",
        "js-beautify": "^1.10.0",
        "lodash": "^4.17.11",
        "primeicons": "^1.0.0",
        "primeng": "^7.1.3",
        "rxjs": "^6.5.2",
        "rxjs-compat": "^6.5.2",
        "zone.js": "^0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.13.9",
        "@angular/cli": "^7.3.9",
        "@angular/compiler-cli": "^7.2.15",
        "@angular/language-service": "^7.2.15",
        "@types/jasmine": "^3.3.12",
        "@types/jasminewd2": "^2.0.6",
        "@types/lodash": "^4.14.132",
        "@types/node": "~11.13.7",
        "codelyzer": "^5.0.1",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "npm-run-all": "^4.1.5",
        "protractor": "^5.4.2",
        "renamer": "^1.1.1",
        "rimraf": "^2.6.2",
        "rollup": "^0.52.1",
        "standard-version": "^6.0.1",
        "ts-node": "~8.1.0",
        "tslint": "~5.16.0",
        "typescript": "^3.2.4"
    }
}
