{
    "name": "seed-images",
    "version": "1.0.0",
    "description": "Angular component for Seed image discovery",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf .tmp && rimraf dist",
        "transpile": "ngc",
        "package": "rollup -c",
        "minify": "./node_modules/uglify-js/bin/uglifyjs dist/bundles/seed-images.umd.js --screw-ie8 --compress --mangle--comments--output dist/bundles/seed-images.umd.min.js",
        "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/* && cpx './readme.md' dist",
        "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AppliedIS/seed-images.git"
    },
    "bugs": {
        "url": "https://github.com/AppliedIS/seed-images/issues"
    },
    "homepage": "https://github.com/AppliedIS/seed-images#readme",
    "author": "Applied Information Sciences",
    "license": "MIT",
    "devDependencies": {
        "@angular/animations": "^5.1.0",
        "@angular/compiler": "^5.1.0",
        "@angular/compiler-cli": "^5.1.0",
        "@angular/router": "^5.1.0",
        "cpx": "^1.5.0",
        "renamer": "^0.6.1",
        "rimraf": "^2.6.2",
        "rollup": "^0.52.1",
        "typescript": "^2.4.2",
        "uglify-js": "^3.2.2"
    },
    "dependencies": {
        "@angular/common": "^5.1.0",
        "@angular/core": "^5.1.0",
        "@angular/forms": "^5.1.0",
        "@angular/http": "^5.1.0",
        "@angular/platform-browser": "^5.1.0",
        "clipboard": "^1.7.1",
        "font-awesome": "^4.7.0",
        "js-beautify": "^1.7.5",
        "primeng": "^5.0.2",
        "rxjs": "^5.5.5",
        "zone.js": "^0.8.18"
    }
}
